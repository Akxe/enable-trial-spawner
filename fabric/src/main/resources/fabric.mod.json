{
  "schemaVersion": 1,
  "id": "enable_trial_spawner",
  "version": "${version}",
  "name": "Enable trial spawner",
  "description": "Allows trial spawners to spawn mobs even if the doMobSpawning gamerule is set to false, matching vanilla mob spawner logic.",
  "authors": [
    "Akxe"
  ],
  "contact": {
    "sources": "https://github.com/Akxe/enable-trial-spawners"
  },
  "license": "MIT",
  "icon": "assets/enable_trial_spawner/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.akxe.enable_trial_spawner.fabric.EnableTrailSpawnerModFabric"
    ],
    "client": [
      "com.akxe.enable_trial_spawner.fabric.client.EnableTrailSpawnerModFabricClient"
    ]
  },
  "mixins": [
    "enable_trial_spawner.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.0",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "architectury": ">=13.0.8",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
